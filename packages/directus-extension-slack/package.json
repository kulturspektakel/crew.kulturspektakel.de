{
  "name": "directus-extension-slack",
  "description": "Please enter a description for your extension",
  "icon": "extension",
  "version": "1.0.0",
  "keywords": [
    "directus",
    "directus-extension",
    "directus-custom-hook"
  ],
  "directus:extension": {
    "type": "hook",
    "path": "../../extensions/directus-extension-slack/index.js",
    "source": "src/index.ts",
    "host": "^9.25.2"
  },
  "scripts": {
    "build": "directus-extension build && cp ./package.json ../../extensions/directus-extension-slack/package.json",
    "dev": "directus-extension build -w --no-minify",
    "link": "directus-extension link"
  },
  "devDependencies": {
    "@directus/extensions-sdk": "10.1.3",
    "@types/node": "^18.16.0",
    "typescript": "^5.0.4"
  }
}
