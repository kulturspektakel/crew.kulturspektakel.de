{
  "name": "directus-extension-react",
  "description": "Please enter a description for your extension",
  "icon": "extension",
  "version": "1.0.0",
  "keywords": [
    "directus",
    "directus-extension",
    "directus-custom-module"
  ],
  "directus:extension": {
    "type": "bundle",
    "path": {
      "app": "../../extensions/directus-extension-react/booking.js",
      "api": "../../extensions/directus-extension-react/endpoints.cjs"
    },
    "entries": [
      {
        "type": "endpoint",
        "name": "react",
        "source": "src/endpoints.ts"
      },
      {
        "type": "module",
        "name": "booking",
        "source": "src/booking.ts"
      }
    ],
    "host": "^9.22.4"
  },
  "scripts": {
    "build": "directus-extension build && cp ./package.json ../../extensions/directus-extension-react/package.json",
    "dev": "directus-extension build -w --no-minify"
  },
  "devDependencies": {
    "@directus/extensions-sdk": "9.26.0",
    "@types/express": "^4.17.17",
    "typescript": "^4.9.5",
    "vue": "^3.2.47"
  }
}
