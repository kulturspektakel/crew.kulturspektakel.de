{
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx"
  },
  "include": [
    "src/booking",
    "src/types",
    "src/vite-env.d.ts",
    "src/vite-env.d.ts",
    "src/utils/graphql.ts",
    "src/utils/graphql.ts",
    "src/utils/Clipboard.tsx",
    "src/utils/Clipboard.tsx",
    "src/utils/useApolloClient.ts",
    "src/utils/useApolloClient.ts",
    "src/utils/useViewerContext.tsx",
    "src/utils/useViewerContext.tsx",
    "src/utils/RelativeDate.tsx",
    "src/utils/RelativeDate.tsx",
    "src/utils/ViewerAvatar.tsx",
    "src/utils/ViewerAvatar.tsx"
  ],
  "references": [{"path": "./tsconfig.node.json"}]
}
