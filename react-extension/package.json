{
  "name": "directus-extension-react",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "graphql": "graphql-codegen --config ./.graphqlrc.yml",
    "build": "tsc && vite build --emptyOutDir && directus-extension build",
    "setup": "directus-extension link ../extensions"
  },
  "directus:extension": {
    "type": "bundle",
    "path": {
      "app": "dist/booking.js",
      "api": "dist/endpoints.cjs"
    },
    "entries": [
      {
        "type": "endpoint",
        "name": "react",
        "source": "extension/endpoints.js"
      },
      {
        "type": "module",
        "name": "react",
        "source": "extension/booking.js"
      }
    ],
    "host": "^9.22.4"
  },
  "dependencies": {
    "@ant-design/icons": "^5.0.1",
    "@apollo/client": "^3.7.7",
    "@googlemaps/react-wrapper": "^1.1.35",
    "antd": "^5.1.7",
    "apollo-link-scalars": "^4.0.1",
    "graphql-scalars": "^1.20.1",
    "jwt-decode": "^3.1.2",
    "react": "18",
    "react-dom": "18",
    "react-virtualized-auto-sizer": "^1.0.7"
  },
  "devDependencies": {
    "@directus/extensions-sdk": "^9.22.4",
    "@graphql-codegen/cli": "^3.0.0",
    "@graphql-codegen/introspection": "^3.0.0",
    "@graphql-codegen/typescript-operations": "^3.0.0",
    "@graphql-codegen/typescript-react-apollo": "^3.3.7",
    "@types/express": "^4.17.17",
    "@types/google.maps": "^3.51.1",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@types/react-virtualized-auto-sizer": "^1.0.1",
    "@vitejs/plugin-react": "^3.1.0",
    "prettier": "^2.8.3",
    "typescript": "^4.9.3",
    "vite": "^4.1.0"
  }
}
